<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="105.0" prefWidth="216.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduler.ConfirmDialog">
    <VBox prefHeight="170.0" prefWidth="322.0">
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #DCDCDC;">
            <Label text="Confirm delete?"/>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0"
              style="-fx-background-color: #BABABA;">
            <Button fx:id="buttonOK" mnemonicParsing="false" onAction="#handleButtonOK" prefWidth="100.0"
                    text="OK"/>
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#handleButtonCancel"
                    prefWidth="100.0" text="Cancel"/>
        </HBox>
    </VBox>
</AnchorPane>
